// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Requests{
  id Int @id @default(autoincrement())
  role String
  address String
}

model Users{
  id Int @id @default(autoincrement())
  role String
  address String
}